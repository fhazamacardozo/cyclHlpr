{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "TrainingReport",
    "type": "object",
    "required": ["date", "sessionDescription"],
    "properties": {
        "date": { "type": "string", "format": "date" },
        "sessionDescription": { "type": "string" },
        "sessionModified": { "type": "boolean" },
        "sessionModification": { "type": "string" },
        "nutrition": { "type": "string" },
        "physicalFeelings": { "type": "string" },
        "breathing": { "type": "string" },
        "rpe": { "type": "string" },
        "mentalFeelings": { "type": "string" },
        "motivacion": { "type": "string" },
        "enfoque": { "type": "string" },
        "sleep": { "type": "string" },
        "otherFactors": { "type": "string" },
        "notes": { "type": "string" }
    },
    "additionalProperties": false
}
